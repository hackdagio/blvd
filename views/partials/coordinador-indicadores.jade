.wrapper.coordinador-view
  .wrapper-content
    .container-fluid
      .row
        //- level 1
        .col-lg-8.nopadding

          //- sublevel 1
          .col-lg-12.nopadding

            //- kpi: general
            .col-md-9
              .kaizen-box.kaizen-kpi.ok.kaizen-self#self-coordinador.h-level-1
                .col-lg-12.nopadding
                  .col-sm-8.nopadding
                    .avatar
                      img.img-circle(src='entel/avatar/30.jpg')
                    .self-info
                      span.who.capitalize Francisca Bilbao
                      span.what.uppercase Coordinador General
                      span.where.uppercase ECC
                  .col-sm-4.high-margin-top
                    .kaizen-box-name.big-size.text-right
                      .glyphicon-kpi
                        span.glyphicon.glyphicon-asterisk
                      span.uppercase Total
                    .kpi.kaizen-box-content.text-right
                      .weighted
                        span.big-size {{ indicadores.Kpi.Ponderado.Valor }}
                      .diff
                        .arrow.positive
                          span.glyphicon.glyphicon-chevron-up
                        .delta
                          span {{ indicadores.Kpi.Ponderado.Delta }}%

            //- kpi: desv q llamadas
            .col-md-3
              .kaizen-box.kaizen-kpi.ok.h-level-1
                .kaizen-box-name
                  .glyphicon-kpi
                    span.glyphicon.glyphicon-asterisk
                  span.uppercase Desviación
                .kpi.kaizen-box-content.text-right
                  .weighted
                    span.normal-size {{ indicadores.Kpi.DesviacionQLlamadas.Valor }}
                  .diff
                    .arrow.negative
                      span.glyphicon.glyphicon-chevron-down
                    .delta
                      span {{ indicadores.Kpi.DesviacionQLlamadas.Delta }}%

          //- sublevel 2
          .col-lg-12.nopadding

            //- kpi: n.s.
            .col-md-3
              .kaizen-box.kaizen-kpi.red-alert.h-level-2
                .kaizen-box-name
                  .glyphicon-kpi
                    span.glyphicon.glyphicon-asterisk
                  span.uppercase Servicio
                  .glyphicon-kpi.right-info
                    button.glyphicon.glyphicon-info-sign(popover-animation='true', popover='{{ indicadores.Kpi.NivelServicio.Valor }}')
                .kpi.kaizen-box-content.text-right
                  .weighted
                    span.normal-size {{ indicadores.Kpi.NivelServicio.Valor }}
                  .diff
                    .arrow.positive
                      span.glyphicon.glyphicon-chevron-up
                    .delta
                      span {{ indicadores.Kpi.NivelServicio.Delta }}%
                  .kpi-info
                    span: | <b>M</b> 29%
                    span: | <b>C</b> 93%

            //- kpi: n.a.
            .col-md-3
              .kaizen-box.kaizen-kpi.red-alert.h-level-2
                .kaizen-box-name
                  .glyphicon-kpi
                    span.glyphicon.glyphicon-asterisk
                  span.uppercase Atención
                  .glyphicon-kpi.right-info
                    button.glyphicon.glyphicon-info-sign(popover-animation='true', popover='{{ indicadores.Kpi.NivelAtencion.Valor }}')
                .kpi.kaizen-box-content.text-right
                  .weighted
                    span.normal-size {{ indicadores.Kpi.NivelAtencion.Valor }}
                  .diff
                    .arrow.positive
                      span.glyphicon.glyphicon-chevron-up
                    .delta
                      span {{ indicadores.Kpi.NivelAtencion.Delta }}%
                  .kpi-info
                    span: | <b>M</b> 29%
                    span: | <b>C</b> 93%


            //- kpi: epa
            .col-md-3
              .kaizen-box.kaizen-kpi.ok.h-level-2
                .kaizen-box-name
                  .glyphicon-kpi
                    span.glyphicon.glyphicon-asterisk
                  span.uppercase EPA
                  .glyphicon-kpi.right-info
                    button.glyphicon.glyphicon-info-sign(popover-animation='true', popover='{{ indicadores.Kpi.EPA.Valor }}')
                .kpi.kaizen-box-content.text-right
                  .weighted
                    span.normal-size {{ indicadores.Kpi.EPA.Valor }}
                  .diff
                    .arrow.positive
                      span.glyphicon.glyphicon-chevron-up
                    .delta
                      span {{ indicadores.Kpi.EPA.Delta }}%
                  .kpi-info
                    span: | <b>M</b> 29%
                    span: | <b>C</b> 93%

            //- kpi: tmo
            .col-md-3
              .kaizen-box.kaizen-kpi.ok.h-level-2
                .kaizen-box-name
                  .glyphicon-kpi
                    span.glyphicon.glyphicon-asterisk
                  span.uppercase TMO
                .kpi.kaizen-box-content.text-right
                  .weighted
                    span.normal-size {{ indicadores.Kpi.TMO }}
                    | <small>seg</small>

        //-- level 2

        //- rendimiento comparado
        .col-lg-4
          .kaizen-box.kaizen-radial-kpi.h-level-3(ng-controller='radialPlotCtrl')
            .kaizen-box-name
              .glyphicon-kpi
                span.glyphicon.glyphicon-asterisk
              span.uppercase Rendimiento comparado
            radial-plot(dsn="dataset_a", point-radius="1.2", labelled="true", compare='dataset_b', free="true", animate="true", play="others")

        //-- level 3

        //- history
        .col-lg-8.nopadding
          .col-md-6
            .kaizen-box.kaizen-people-ranking.h-level-3
              .kaizen-box-name
                .glyphicon-kpi
                  span.glyphicon.glyphicon-asterisk
                span.uppercase Mejores supervisores
                .glyphicon-kpi.right-info
                  button.glyphicon.glyphicon-question-sign(popover-animation='true', popover='texto')
              .people-ranking.kaizen-box-content(ng-controller='ProgressDemoCtrl')
                .each-people.col-md-12.nopadding
                  .each-avatar.col-xs-2.nopadding
                    img.img-circle.text-center(src='entel/avatar/4.jpg')
                  .each-name-and-kpi.col-xs-9
                    h4 {{ nameData[0] }}
                    progressbar(max='max', value='97', type='success')
                      i 97,4%
                  .each-social-interactions.col-xs-1.nopadding
                    .each-glyphicon-container
                      span.glyphicon.glyphicon-share-alt
                .each-people.col-md-12.nopadding
                  .each-avatar.col-xs-2.nopadding
                    img.img-circle(src='entel/avatar/3.jpg')
                  .each-name-and-kpi.col-xs-9
                    h4 {{ nameData[1] }}
                    progressbar(max='max', value='94', type='success')
                      i 94,5%
                  .each-social-interactions.col-xs-1.nopadding
                    .each-glyphicon-container
                      span.glyphicon.glyphicon-share-alt
                .each-people.col-md-12.nopadding
                  .each-avatar.col-xs-2.nopadding
                    img.img-circle(src='entel/avatar/2.jpg')
                  .each-name-and-kpi.col-xs-9
                    h4 {{ nameData[2] }}
                    progressbar(max='max', value='91', type='success')
                      i 91,3%
                  .each-social-interactions.col-xs-1.nopadding
                    .each-glyphicon-container
                      span.glyphicon.glyphicon-share-alt
                .each-people.col-md-12.nopadding
                  .each-avatar.col-xs-2.nopadding
                    img.img-circle(src='entel/avatar/1.jpg')
                  .each-name-and-kpi.col-xs-9
                    h4 {{ nameData[3] }}
                    progressbar(max='max', value='90', type='success')
                      i 90,1%
                  .each-social-interactions.col-xs-1.nopadding
                    .each-glyphicon-container
                      span.glyphicon.glyphicon-share-alt

          .col-md-6
            .kaizen-box.kaizen-people-ranking.h-level-3
              .kaizen-box-name
                .glyphicon-kpi
                  span.glyphicon.glyphicon-asterisk
                span.uppercase Mejores ejecutivos
                .glyphicon-kpi.right-info
                  button.glyphicon.glyphicon-question-sign(popover-animation='true', popover='texto')
              .people-ranking.kaizen-box-content(ng-controller='ProgressDemoCtrl')
                .each-people.col-md-12.nopadding
                  .each-avatar.col-xs-2.nopadding
                    img.img-circle.text-center(src='entel/avatar/5.jpg')
                  .each-name-and-kpi.col-xs-9
                    h4 {{ ejecutivosData[0] }}
                    progressbar(max='max', value='99.3', type='success')
                      i 99,3%
                  .each-social-interactions.col-xs-1.nopadding
                    .each-glyphicon-container
                      span.glyphicon.glyphicon-share-alt
                .each-people.col-md-12.nopadding
                  .each-avatar.col-xs-2.nopadding
                    img.img-circle(src='entel/avatar/3.jpg')
                  .each-name-and-kpi.col-xs-9
                    h4 {{ ejecutivosData[1] }}
                    progressbar(max='max', value='98.5', type='success')
                      i 98,5%
                  .each-social-interactions.col-xs-1.nopadding
                    .each-glyphicon-container
                      span.glyphicon.glyphicon-share-alt
                .each-people.col-md-12.nopadding
                  .each-avatar.col-xs-2.nopadding
                    img.img-circle(src='entel/avatar/2.jpg')
                  .each-name-and-kpi.col-xs-9
                    h4 {{ ejecutivosData[2] }}
                    progressbar(max='max', value='97.4', type='success')
                      i 97,4%
                  .each-social-interactions.col-xs-1.nopadding
                    .each-glyphicon-container
                      span.glyphicon.glyphicon-share-alt
                .each-people.col-md-12.nopadding
                  .each-avatar.col-xs-2.nopadding
                    img.img-circle(src='entel/avatar/1.jpg')
                  .each-name-and-kpi.col-xs-9
                    h4 {{ ejecutivosData[3] }}
                    progressbar(max='max', value='97.2', type='success')
                      i 97,2%
                  .each-social-interactions.col-xs-1.nopadding
                    .each-glyphicon-container
                      span.glyphicon.glyphicon-share-alt


        //- news
        .col-lg-4
          .kaizen-box.kaizen-latest-news.h-level-3
            .kaizen-box-name
              .glyphicon-kpi
                span.glyphicon.glyphicon-asterisk
              span.uppercase Últimas noticias
            .latest-news.kaizen-box-content
              .each-news.latest
                .col-sm-12.nopadding
                  .col-lg-2
                    img.cover-news(src='entel/images/news/winners.jpg')
                  .col-lg-9.nopadding
                    h2 Ganadores de Noviembre
                    h5 Hace 2 días · por Área de Ventas
                  .col-lg-1.nopadding
                    img.guino(src='entel/images/guiño.svg')
              .each-news
                .col-sm-12.nopadding
                  .col-lg-2
                    img.cover-news(src='entel/images/news/smile.jpg')
                  .col-lg-9.nopadding
                    h2 Motivate con Sonrisas
                    h5 Hace 2 días · por Gerencia
                  .col-lg-1.nopadding
                    img.guino(src='entel/images/guiño.svg')
              .each-news
                .col-sm-12.nopadding
                  .col-lg-2
                    img.cover-news(src='entel/images/news/sell.jpg')
                  .col-lg-9.nopadding
                    h2 Juntos vendemos más
                    h5 Hace 3 días · por Área de Ventas
                  .col-lg-1.nopadding
                    img.guino(src='entel/images/guiño.svg')
