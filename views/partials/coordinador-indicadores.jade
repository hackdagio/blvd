.wrapper.coordinador-view
  .wrapper-content
    .container-fluid
      .row
        // level 1
        .col-lg-8.nopadding

          // sublevel 1
          .col-lg-12.nopadding

            // kpi: general
            .col-md-9
              .kaizen-box.kaizen-kpi.ok.kaizen-self.h-level-1
                .col-lg-8
                  .avatar
                    img.img-circle(src='entel/avatar/30.jpg')
                  .self-info
                    span.who.capitalize Francisca bilbao
                    span.what.uppercase Coordinador General
                    span.where.uppercase Entel Call Center
                .col-lg-4
                  .kpi.text-right
                    .name.big-size
                      .glyphicon-kpi
                        span.glyphicon.glyphicon-asterisk
                      span.uppercase Total
                    .weighted
                      span.big-size {{ indicadores.Kpi.Ponderado.Valor }}
                    .diff
                      .arrow.positive
                        span.glyphicon.glyphicon-chevron-up
                      .delta
                        span {{ indicadores.Kpi.Ponderado.Delta }}%

            // kpi: desv q llamadas
            .col-md-3
              .kaizen-box.kaizen-kpi.ok.h-level-1
                .kpi
                  .name
                    .glyphicon-kpi
                      span.glyphicon.glyphicon-asterisk
                    span.uppercase Desv. Llamadas
                    .glyphicon-kpi.right-info
                      button.glyphicon.glyphicon-info-sign(popover-animation='true', popover='{{ indicadores.Kpi.DesviacionQLlamadas.Valor }}')
                  .weighted.text-right
                    span.normal-size {{ indicadores.Kpi.DesviacionQLlamadas.Valor }}
                  .diff.text-right
                    .arrow.negative
                      span.glyphicon.glyphicon-chevron-down
                    .delta
                      span {{ indicadores.Kpi.DesviacionQLlamadas.Delta }}%

          // sublevel 2
          .col-lg-12.nopadding

            // kpi: n.s.
            .col-md-3
              .kaizen-box.kaizen-kpi.red-alert.h-level-2
                .kpi
                  .name
                    .glyphicon-kpi
                      span.glyphicon.glyphicon-asterisk
                    span.uppercase Servicio
                    .glyphicon-kpi.right-info
                      button.glyphicon.glyphicon-info-sign(popover-animation='true', popover='{{ indicadores.Kpi.NivelServicio.Valor }}')
                  .weighted.text-right
                    span.normal-size {{ indicadores.Kpi.NivelServicio.Valor }}
                  .diff.text-right
                    .arrow.positive
                      span.glyphicon.glyphicon-chevron-up
                    .delta
                      span {{ indicadores.Kpi.NivelServicio.Delta }}%

            // kpi: n.a.
            .col-md-3
              .kaizen-box.kaizen-kpi.red-alert.h-level-2
                .kpi
                  .name
                    .glyphicon-kpi
                      span.glyphicon.glyphicon-asterisk
                    span.uppercase Atención
                    .glyphicon-kpi.right-info
                      button.glyphicon.glyphicon-info-sign(popover-animation='true', popover='{{ indicadores.Kpi.NivelAtencion.Valor }}')
                  .weighted.text-right
                    span.normal-size {{ indicadores.Kpi.NivelAtencion.Valor }}
                  .diff.text-right
                    .arrow.positive
                      span.glyphicon.glyphicon-chevron-up
                    .delta
                      span {{ indicadores.Kpi.NivelAtencion.Delta }}%

            // kpi: epa
            .col-md-3
              .kaizen-box.kaizen-kpi.ok.h-level-2
                .kpi
                  .name
                    .glyphicon-kpi
                      span.glyphicon.glyphicon-asterisk
                    span.uppercase EPA
                    .glyphicon-kpi.right-info
                      button.glyphicon.glyphicon-info-sign(popover-animation='true', popover='{{ indicadores.Kpi.EPA.Valor }}')
                  .weighted.text-right
                    span.normal-size {{ indicadores.Kpi.EPA.Valor }}
                  .diff.text-right
                    .arrow.positive
                      span.glyphicon.glyphicon-chevron-up
                    .delta
                      span {{ indicadores.Kpi.EPA.Delta }}%

            // kpi: tmo
            .col-md-3
              .kaizen-box.kaizen-kpi.ok.h-level-2
                .kpi
                  .name
                    .glyphicon-kpi
                      span.glyphicon.glyphicon-asterisk
                    span.uppercase TMO
                    .glyphicon-kpi.right-info
                      button.glyphicon.glyphicon-info-sign(popover-animation='true', popover='{{ indicadores.Kpi.TMO }}')
                  .weighted.text-right
                    span.normal-size {{ indicadores.Kpi.TMO }}
                    | <small> seg</small>

        // level 2

        // rendimiento comparado
        .col-lg-4
          .kaizen-box.kaizen-radial-kpi.h-level-3(ng-controller='radialPlotCtrl')
            .name.big-size
              .glyphicon-kpi
                span.glyphicon.glyphicon-asterisk
              span.uppercase Rendimiento comparado
              .glyphicon-kpi.right-info
                button.glyphicon.glyphicon-info-sign(popover-animation='true', popover='texto')
            radial-plot(dsn="dataset_a", point-radius="1.2", labelled="true", compare='dataset_b', free="true", animate="true", play="others")

        // level 3

        // history
        .col-lg-8.nopadding
          .col-md-6
            .kaizen-box.kaizen-people-ranking.h-level-3
              .name
                .glyphicon-kpi
                  span.glyphicon.glyphicon-asterisk
                span.uppercase Mejores supervisores
                .glyphicon-kpi.right-info
                  button.glyphicon.glyphicon-info-sign(popover-animation='true', popover='texto')

          .col-md-6
            .kaizen-box.kaizen-people-ranking.h-level-3
              .name
                .glyphicon-kpi
                  span.glyphicon.glyphicon-asterisk
                span.uppercase Mejores ejecutivos
                .glyphicon-kpi.right-info
                  button.glyphicon.glyphicon-info-sign(popover-animation='true', popover='texto')



        // habilidades
        .col-lg-4
          .kaizen-box.kaizen-latest-news.h-level-3
            .name
              .glyphicon-kpi
                span.glyphicon.glyphicon-asterisk
              span.uppercase Últimas noticias
            .newsfeed
              .news.latest
                h2 Ganadores de Noviembre
                h5 Hace 2 días · por Área de Ventas
              .news
                h2 Motivate con Sonrisas
                h5 Hace 3 días · por Gerencia
              .news
                h2 Juntos vendemos más
                h5 Hace 3 días · por Área de Ventas

script.
  $("#menu-toggle").click(function(e) {
  e.preventDefault();
  $("#wrapper").toggleClass("toggled");
  });



