.wrapper.coordinador-view
  .wrapper-content
    .container-fluid
      .row
        // level 1
        .col-lg-8.nopadding

          // sublevel 1
          .col-lg-12.nopadding

            // kpi: general
            .col-md-9
              .kaizen-box.kaizen-kpi.ok.kaizen-self#self-coordinador.h-level-1
                .col-lg-8
                  .avatar
                    img.img-circle(src='entel/avatar/30.jpg')
                  .self-info
                    span.who.capitalize Francisca Bilbao
                    span.what.uppercase Coordinador General
                    span.where.uppercase ECC
                .col-lg-4
                  .kaizen-box-name.big-size.text-right
                    .glyphicon-kpi
                      span.glyphicon.glyphicon-asterisk
                    span.uppercase Total
                  .kpi.kaizen-box-content.text-right
                    .weighted
                      span.big-size {{ indicadores.Kpi.Ponderado.Valor }}
                    .diff
                      .arrow.positive
                        span.glyphicon.glyphicon-chevron-up
                      .delta
                        span {{ indicadores.Kpi.Ponderado.Delta }}%

            // kpi: desv q llamadas
            .col-md-3
              .kaizen-box.kaizen-kpi.ok.h-level-1
                .kaizen-box-name
                  .glyphicon-kpi
                    span.glyphicon.glyphicon-asterisk
                  span.uppercase Desv. Llamadas
                  .glyphicon-kpi.right-info
                    button.glyphicon.glyphicon-question-sign(popover-animation='true', popover='{{ indicadores.Kpi.DesviacionQLlamadas.Valor }}')
                .kpi.kaizen-box-content
                  .weighted.text-right
                    span.normal-size {{ indicadores.Kpi.DesviacionQLlamadas.Valor }}
                  .diff.text-right
                    .arrow.negative
                      span.glyphicon.glyphicon-chevron-down
                    .delta
                      span {{ indicadores.Kpi.DesviacionQLlamadas.Delta }}%

          // sublevel 2
          .col-lg-12.nopadding

            // kpi: n.s.
            .col-md-3
              .kaizen-box.kaizen-kpi.red-alert.h-level-2
                .kaizen-box-name
                  .glyphicon-kpi
                    span.glyphicon.glyphicon-asterisk
                  span.uppercase Servicio
                  .glyphicon-kpi.right-info
                    button.glyphicon.glyphicon-question-sign(popover-animation='true', popover='{{ indicadores.Kpi.NivelServicio.Valor }}')
                .kpi.kaizen-box-content
                  .weighted.text-right
                    span.normal-size {{ indicadores.Kpi.NivelServicio.Valor }}
                  .diff.text-right
                    .arrow.positive
                      span.glyphicon.glyphicon-chevron-up
                    .delta
                      span {{ indicadores.Kpi.NivelServicio.Delta }}%

            // kpi: n.a.
            .col-md-3
              .kaizen-box.kaizen-kpi.red-alert.h-level-2
                .kaizen-box-name
                  .glyphicon-kpi
                    span.glyphicon.glyphicon-asterisk
                  span.uppercase Atenci√≥n
                  .glyphicon-kpi.right-info
                    button.glyphicon.glyphicon-question-sign(popover-animation='true', popover='{{ indicadores.Kpi.NivelAtencion.Valor }}')
                .kpi.kaizen-box-content
                  .weighted.text-right
                    span.normal-size {{ indicadores.Kpi.NivelAtencion.Valor }}
                  .diff.text-right
                    .arrow.positive
                      span.glyphicon.glyphicon-chevron-up
                    .delta
                      span {{ indicadores.Kpi.NivelAtencion.Delta }}%

            // kpi: epa
            .col-md-3
              .kaizen-box.kaizen-kpi.ok.h-level-2
                .kaizen-box-name
                  .glyphicon-kpi
                    span.glyphicon.glyphicon-asterisk
                  span.uppercase EPA
                  .glyphicon-kpi.right-info
                    button.glyphicon.glyphicon-question-sign(popover-animation='true', popover='{{ indicadores.Kpi.EPA.Valor }}')
                .kpi.kaizen-box-content
                  .weighted.text-right
                    span.normal-size {{ indicadores.Kpi.EPA.Valor }}
                  .diff.text-right
                    .arrow.positive
                      span.glyphicon.glyphicon-chevron-up
                    .delta
                      span {{ indicadores.Kpi.EPA.Delta }}%

            // kpi: tmo
            .col-md-3
              .kaizen-box.kaizen-kpi.ok.h-level-2
                .kaizen-box-name
                  .glyphicon-kpi
                    span.glyphicon.glyphicon-asterisk
                  span.uppercase TMO
                  .glyphicon-kpi.right-info
                    button.glyphicon.glyphicon-question-sign(popover-animation='true', popover='{{ indicadores.Kpi.TMO }}')
                .kpi.kaizen-box-content
                  .weighted.text-right
                    span.normal-size {{ indicadores.Kpi.TMO }}
                    | <small> seg</small>

        //- level 2

        // rendimiento comparado
        .col-lg-4
          .kaizen-box.kaizen-radial-kpi.h-level-3(ng-controller='radialPlotCtrl')
            .kaizen-box-name
              .glyphicon-kpi
                span.glyphicon.glyphicon-asterisk
              span.uppercase Rendimiento comparado
            radial-plot(dsn="dataset_a", point-radius="1.2", labelled="true", compare='dataset_b', free="true", animate="true", play="others")

        //- level 3

        // history
        .col-lg-8.nopadding
          .col-md-6
            .kaizen-box.kaizen-people-ranking.h-level-3
              .kaizen-box-name
                .glyphicon-kpi
                  span.glyphicon.glyphicon-asterisk
                span.uppercase Mejores supervisores
